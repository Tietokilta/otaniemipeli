{
  "name": "frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e ../../.env -- next dev",
    "build": "dotenv -e ../../.env -- next build",
    "start": "dotenv -e ../../.env -- next start",
    "lint": "eslint ..",
    "lint:fix": "eslint .. --ext .js,.ts,.tsx --fix",
    "format": "prettier --write --list-different \"**/*.{js,ts,ts,tsx,md,json}\"",
    "format:check": "prettier --check \"**/*.{js,ts,ts,tsx,md,json}\"",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf .next node_modules && pnpm install",
    "precommit": "pnpm typecheck && pnpm lint && pnpm format && git add .. && echo '\nðŸ¤©  Pre-commit checks completed! ðŸ¤©'"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "fast-deep-equal": "^3.1.3",
    "lucide-react": "^0.483.0",
    "next": "16.1.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "socket.io-client": "^4.8.3"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^16.1.6",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^20.19.32",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "dotenv": "^17.2.4",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.1.6",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "prettier": "^3.8.1",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3"
  }
}
